<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.atsoft.dasheng.app.mapper.ContractMachineMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.atsoft.dasheng.app.entity.ContractMachine">
        <id column="contract_id" property="contractId" />
        <result column="purchase_unit_id" property="purchaseUnitId" />
        <result column="signing_site" property="signingSite" />
        <result column="supply_unit_id" property="supplyUnitId" />
        <result column="equipment" property="equipment" />
        <result column="project_name" property="projectName" />
        <result column="contract_amount" property="contractAmount" />
        <result column="payment" property="payment" />
        <result column="effective_date" property="effectiveDate" />
        <result column="amount_one" property="amountOne" />
        <result column="amount_two" property="amountTwo" />
        <result column="run_date" property="runDate" />
        <result column="amount_three" property="amountThree" />
        <result column="pace_delivery" property="paceDelivery" />
        <result column="deliverer_name" property="delivererName" />
        <result column="delivery_address" property="deliveryAddress" />
        <result column="transportation_mode" property="transportationMode" />
        <result column="installation_location" property="installationLocation" />
        <result column="acceptance_time_one" property="acceptanceTimeOne" />
        <result column="acceptance_time_two" property="acceptanceTimeTwo" />
        <result column="acceptance_time_three" property="acceptanceTimeThree" />
        <result column="receive_location" property="receiveLocation" />
        <result column="charge_standard" property="chargeStandard" />
        <result column="warranty_period_one" property="warrantyPeriodOne" />
        <result column="warranty_period_two" property="warrantyPeriodTwo" />
        <result column="warranty_period_three" property="warrantyPeriodThree" />
        <result column="warranty_period_four" property="warrantyPeriodFour" />
        <result column="warranty_period_five" property="warrantyPeriodFive" />
        <result column="party_a_name" property="partyAName" />
        <result column="Party_a_time" property="partyATime" />
        <result column="Party_b_name" property="partyBName" />
        <result column="Party_b_time" property="partyBTime" />
        <result column="contract_name" property="contractName" />
        <result column="creation_time" property="creationTime" />
        <result column="note" property="note" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">   creation_time AS "creationTime" , creation_time AS "creationTime" , note AS "note", contract_name AS "contractName",
        contract_id AS "contractId", purchase_unit_id AS "purchaseUnitId", supply_unit_id AS "supplyUnitId", equipment AS "equipment", project_name AS "projectName", contract_amount AS "contractAmount", payment AS "payment", effective_date AS "effectiveDate", amount_one AS "amountOne", amount_two AS "amountTwo", run_date AS "runDate", amount_three AS "amountThree", pace_delivery AS "paceDelivery", deliverer_name AS "delivererName", delivery_address AS "deliveryAddress", transportation_mode AS "transportationMode", installation_location AS "installationLocation", acceptance_time_one AS "acceptanceTimeOne", acceptance_time_two AS "acceptanceTimeTwo", acceptance_time_three AS "acceptanceTimeThree", receive_location AS "receiveLocation", charge_standard AS "chargeStandard", warranty_period_one AS "warrantyPeriodOne", warranty_period_two AS "warrantyPeriodTwo", warranty_period_three AS "warrantyPeriodThree", warranty_period_four AS "warrantyPeriodFour", warranty_period_five AS "warrantyPeriodFive", party_a_name AS "partyAName", Party_a_time AS "partyATime", Party_b_name AS "partyBName", Party_b_time AS "partyBTime"
    </sql>


    <select id="customList" resultType="cn.atsoft.dasheng.app.model.result.ContractMachineResult" parameterType="cn.atsoft.dasheng.app.model.params.ContractMachineParam">
        select
        <include refid="Base_Column_List"/>
        from daoxin_contract_machine where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.atsoft.dasheng.app.model.params.ContractMachineParam">
        select
        <include refid="Base_Column_List"/>
        from daoxin_contract_machine where 1 = 1
    </select>

    <select id="customPageList" resultType="cn.atsoft.dasheng.app.model.result.ContractMachineResult" parameterType="cn.atsoft.dasheng.app.model.params.ContractMachineParam">
        select
        <include refid="Base_Column_List"/>
        from daoxin_contract_machine where 1 = 1
        <if test="paramCondition.contractName != null and paramCondition.contractName != ''">
            and contract_name like CONCAT('%',#{paramCondition.contractName},'%')
        </if>
        <if test="paramCondition.creationTime != null and paramCondition.creationTime != ''">
            and creation_time like CONCAT('%',#{paramCondition.creationTime},'%')
        </if>
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.atsoft.dasheng.app.model.params.ContractMachineParam">
        select
        <include refid="Base_Column_List"/>
        from daoxin_contract_machine where 1 = 1
    </select>

</mapper>
