<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.atsoft.dasheng.form.mapper.ActivitiProcessLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.atsoft.dasheng.form.entity.ActivitiProcessLog">
        <id column="log_id" property="logId" />
        <result column="peocess_id" property="peocessId" />
        <result column="setps_id" property="setpsId" />
        <result column="status" property="status" />
        <result column="display" property="display" />
        <result column="create_user" property="createUser" />
        <result column="update_user" property="updateUser" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        log_id AS "logId", peocess_id AS "peocessId", setps_id AS "setpsId", status AS "status", display AS "display", create_user AS "createUser", update_user AS "updateUser", create_time AS "createTime", update_time AS "updateTime"
    </sql>


    <select id="customList" resultType="cn.atsoft.dasheng.form.model.result.ActivitiProcessLogResult" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessLogParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_log where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessLogParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_log where 1 = 1
    </select>

    <select id="customPageList" resultType="cn.atsoft.dasheng.form.model.result.ActivitiProcessLogResult" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessLogParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_log where 1 = 1
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessLogParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_log where 1 = 1
    </select>

</mapper>
