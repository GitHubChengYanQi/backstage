<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.atsoft.dasheng.form.mapper.ActivitiProcessTaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.atsoft.dasheng.form.entity.ActivitiProcessTask">
        <id column="process_task_id" property="processTaskId" />
        <result column="process_id" property="processId" />
        <result column="display" property="display" />
        <result column="task_name" property="taskName"/>
        <result column="create_user" property="createUser" />
        <result column="update_user" property="updateUser" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        process_task_id AS "processTaskId",task_name AS "taskName", process_id AS "processId", display AS "display", create_user AS "createUser", update_user AS "updateUser", create_time AS "createTime", update_time AS "updateTime"
    </sql>


    <select id="customList" resultType="cn.atsoft.dasheng.form.model.result.ActivitiProcessTaskResult" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessTaskParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_task where 1 = 1
    </select>

    <select id="customMapList" resultType="map" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessTaskParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_task where 1 = 1
    </select>

    <select id="customPageList" resultType="cn.atsoft.dasheng.form.model.result.ActivitiProcessTaskResult" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessTaskParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_task where 1 = 1
    </select>

    <select id="customPageMapList" resultType="map" parameterType="cn.atsoft.dasheng.form.model.params.ActivitiProcessTaskParam">
        select
        <include refid="Base_Column_List"/>
        from activiti_process_task where 1 = 1
    </select>

</mapper>
